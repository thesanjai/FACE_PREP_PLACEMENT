   "source": [
    "import math\n",
    "\n",
    "def catalan(n):\n",
    "    return math.comb(2 * n, n) // (n + 1)\n",
    "\n",
    "# Example usage\n",
    "n = int(input(\"Enter n: \"))\n",
    "result = catalan(n)\n",
    "print(f\"(2n C n) / (n+1) for n = {n} is {result}\")\n"
   ]
  }
